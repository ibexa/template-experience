{
  "name": "@qntmgroup/app-switcher",
  "version": "1.0.5",
  "repository": {
    "url": "git+https://github.com/qntmgroup/qntm-ui.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
